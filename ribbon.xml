<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" loadImage="GetImage" >
	<ribbon>
		<tabs>
			<tab id="bianji1" label="项目管理">
				<group id="groupbianji" label="标记管理">
					<button id="update_tittle" label="更新标题标记" size="large" onAction="update_tittle" image="update_title.png" />
					<menu id="rxboxCustom_add" label="新增标记">	
						<button id="playlist_add_add" label="【未开展】" onAction="playlist_add_add" />
						<button id="Playlist_add_kaizhanzhong" label="【开展中】"  onAction="Playlist_add_kaizhanzhong" />
						<button id="Playlist_add_weiqueren" label="【未确认】" onAction="Playlist_add_weiqueren" />
						<button id="Playlist_add_zuofei" label="【作废】" onAction="Playlist_add_zuofei" />
						<button id="Playlist_add_daisheji" label="【待设计】" onAction="Playlist_add_daisheji" />
						<button id="Playlist_add_weizhuan" label="【未转】" onAction="Playlist_add_weizhuan" />
						<button id="Playlist_add_hebing" label="【合并】" onAction="Playlist_add_hebing" />
						<button id="Playlist_add_yizhuan" label="【已转】" onAction="Playlist_add_yizhuan" />
						<button id="Playlist_add_zanbukaizhan" label="【暂不开展】" onAction="Playlist_add_zanbukaizhan" />
						<button id="Playlist_add_yizhuanxubuchong" label="【已转需补充】" onAction="Playlist_add_yizhuanxubuchong" />
						<button id="Playlist_add_yiwancheng" label="【已完成】" onAction="Playlist_add_yiwancheng" />
						<button id="Playlist_add_p_work_n" label="项目工作（内部）" onAction="Playlist_add_p_work_n" />
						<button id="Playlist_add_p_work_w" label="项目工作（外部）" onAction="Playlist_add_p_work_w" />
					</menu>
					<menu id="rxboxCustom_del" label="删除标记">
						<button id="playlist_del_add" label="【未开展】" onAction="playlist_del_add" />
						<button id="Playlist_del_kaizhanzhong" label="【开展中】"  onAction="Playlist_del_kaizhanzhong" />
						<button id="Playlist_del_weiqueren" label="【未确认】" onAction="Playlist_del_weiqueren" />
						<button id="Playlist_del_zuofei" label="【作废】" onAction="Playlist_del_zuofei" />
						<button id="Playlist_del_daisheji" label="【待设计】" onAction="Playlist_del_daisheji" />
						<button id="Playlist_del_weizhuan" label="【未转】" onAction="Playlist_del_weizhuan" />
						<button id="Playlist_del_hebing" label="【合并】" onAction="Playlist_del_hebing" />
						<button id="Playlist_del_yizhuan" label="【已转】" onAction="Playlist_del_yizhuan" />
						<button id="Playlist_del_zanbukaizhan" label="【暂不开展】" onAction="Playlist_del_zanbukaizhan" />
						<button id="Playlist_del_yizhuanxubuchong" label="【已转需补充】" onAction="Playlist_del_yizhuanxubuchong" />
						<button id="Playlist_del_yiwancheng" label="【已完成】" onAction="Playlist_del_yiwancheng" />
						<button id="Playlist_del_p_work_n" label="项目工作（内部）" onAction="Playlist_del_p_work_n" />
						<button id="Playlist_del_p_work_w" label="项目工作（外部）" onAction="Playlist_del_p_work_w" />
					</menu>
					<button id="Del_all_tags" label="删除所有标记" onAction="Del_all_tags" />
				</group>
				<group id="groupzonghe" label="综合管理">
					<button id="allin_xml" label="查看xml" size="normal" onAction="allin_xml" image="playlist_add.png" />
				</group>
				<group id="grouppage" label="页面管理">
					<button id="page_a4" label="A4页面设置" size="normal" onAction="page_a4" image="playlist_add.png" />
					<button id="Del_none" label="删空数据" size="normal" onAction="Del_none" image="playlist_add.png" />
					<button id="Tongyi_data" label="统一数据位置（A4宽）" size="normal" onAction="Tongyi_data" image="playlist_add.png" />
				</group>
				<group id="riji" label="日记管理">
					<button id="riji_create_page" label="创建日记页（MyJournal.Notebook）" size="normal" onAction="riji_create_page" image="playlist_add.png" />
					<button id="create_my_riji" label="项目管理日记" size="normal" onAction="create_my_riji" image="playlist_add.png" />
				</group>
				<group id="ribao" label="日报管理">
					<button id="create_my_ribao_kong" label="每日日报（空模板）" size="normal" onAction="create_my_ribao_kong" image="playlist_add.png" />
					<button id="create_my_ribao_quan" label="每日日报（全）" size="normal" onAction="create_my_ribao_quan" image="playlist_add.png" />
					<button id="create_my_ribao_dan" label="每日日报（报工）" size="normal" onAction="create_my_ribao_dan" image="playlist_add.png" />
					<button id="create_my_ribao_quan_leater" label="明日日报（报工）" size="normal" onAction="create_my_ribao_quan_leater" image="playlist_add.png" />
					<button id="create_my_ribao_quan_all" label="生成批量日报（全）" size="normal" onAction="create_my_ribao_quan_all" image="playlist_add.png" />
				</group>
				<group id="project" label="项目内容管理">
					<dropDown id="project_name" label="项目名称（笔记本）" getItemCount="GetItemCount"  getItemID="GetItemID" getItemLabel="GetItemLabel" onAction="GetonAction"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
	<contextMenus>
		<contextMenu idMso="ContextMenuText">
			<button id="copy_chun" label="复制为纯文本" onAction="copy_chun" insertBeforeMso="Cut"/>
			<menuSeparator id="project_1" insertBeforeMso="Cut"/>
		</contextMenu>
	</contextMenus>
</customUI>